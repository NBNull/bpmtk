<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="ConsoleApp"
                   namespace="Bpmtk.Engine">

  <class name="ProcessInstance" table="bpm_proc_inst"
         dynamic-update="true">
    <id name="Id" column="id">
      <generator class="identity"/>
    </id>

    <property name="Name" column="`name`" length="100" not-null="true"/>

    <set name="Variables" inverse="true" cascade="all-delete-orphan">
      <key column="proc_inst_id"/>
      <one-to-many class="Variable"/>
    </set>

    <set name="Tokens" inverse="true" cascade="all-delete-orphan">
      <key column="proc_inst_id"/>
      <one-to-many class="Token"/>
    </set>

    <!--<set name="identityLinks" inverse="true" cascade="all-delete-orphan"
         access="field">
      <key column="proc_inst_id"/>
      <one-to-many class="InstanceIdentityLink"/>
    </set>-->
  </class>

</hibernate-mapping>
